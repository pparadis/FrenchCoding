{
  "name": "FrenchCoding.ChromeScreenshot",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Prends des screenshots de vos tabs",
  "background": {
    "scripts": ["script.js", "jquery.min.js"]
  },
    "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Capture tab" 
  },
  "permissions": [
	  "tabs",
    "<all_urls>"
  ]
}